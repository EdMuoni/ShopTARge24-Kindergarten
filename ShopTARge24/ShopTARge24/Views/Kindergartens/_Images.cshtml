@model ShopTARge24.Models.Kindergartens.ImageViewModel

@{
    var photoPaths = "~/multipleFileUpload/" + (Model.Filepath ?? "noimage.jpg");
}

<div class="form-group row col-sm-4 offset-2">
    <div>@Model.Filepath</div>
    <img class="card-image-top" src="@photoPaths" asp-append-version="true" />
</div>